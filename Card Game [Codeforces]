Question Link: https://codeforces.com/problemset/problem/2104/C
Solution:
#include <bits/stdc++.h>
using namespace std;

bool func(int n, int x, int y){
    if(x==0) return y ==n-1;
    if(x == n-1) return y!=0;
    return x>y;
}
int main(){
    int t;
    cin >>t;
    while(t--){
        int n;
        string s;
        cin >> n >> s;
        bool good = false;
        for(int i =0; i <n;i++){
            if(s[i] != 'A') continue;
           bool move = true;
            for(int j =0;j <n;j++){
                if(s[j] == 'B' && func(n,j,i)){move= false;}
            }

if(move){good = true;}
        }
        if (good) cout << "Alice" << endl;
        else cout << "Bob" << endl;
    }
    return 0;

}
   
